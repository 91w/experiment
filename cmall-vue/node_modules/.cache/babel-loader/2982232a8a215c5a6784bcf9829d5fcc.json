 axios\r\n// 导入极验\r\n\r\n// 绑定到原型\r\nVue.prototype.$initGeet = require('@/assets/gt.js')\r\n\r\n// 全局函数及变量\r\nimport Global from './Global'\r\nVue.use(Global)\r\n\r\n// 全局请求拦截器\r\naxios.interceptors.request.use(\r\n  config => {\r\n    let token = window.localStorage.getItem('token')\r\n    if (token) {\r\n      //将token放到请求头发送给服务器,将tokenkey放在请求头中\r\n      config.headers.satoken = token\r\n      //也可以这种写法\r\n      // config.headers['accessToken'] = Token;\r\n    }\r\n    return config\r\n  },\r\n  error => {\r\n    // Do something with request error\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n//跳转页面时返回顶部\r\nrouter.afterEach((to, from, next) => {\r\n  window.scrollTo(0, 0)\r\n})\r\n\r\n// 全局拦截器,在进入需要用户权限的页面前校验是否已经登录\r\nrouter.beforeResolve((to, from, next) => {\r\n  const loginUser = store.state.user.user\r\n  // 判断路由是否设置相应校验用户权限\r\n  if (to.meta.requireAuth) {\r\n    if (!loginUser) {\r\n      // 没有登录，转到登录界面\r\n      router.push({\r\n        name: 'Login'\r\n      })\r\n      if (from.name == null) {\r\n        //此时，是在页面没有加载，直接在地址栏输入链接，进入需要登录验证的页面\r\n        next('/')\r\n        return\r\n      }\r\n      // 终止导航\r\n      next(false)\r\n      return\r\n    }\r\n  }\r\n  next()\r\n})\r\n\r\n// 相对时间过滤器,把时间戳转换成时间\r\n// 格式: 2020-02-25 21:43:23\r\nVue.filter('dateFormat', cjsj => {\r\n  if (cjsj / 10000000000 <= 1) {\r\n    cjsj = cjsj * 1000\r\n  }\r\n  var date = new Date(cjsj) //时间戳为10位需*1000，时间戳为13位的话不需乘1000\r\n  var Y = date.getFullYear() + '-'\r\n  var M =\r\n    (date.getMonth() + 1 < 10\r\n      ? '0' + (date.getMonth() + 1)\r\n      : date.getMonth() + 1) + '-'\r\n  var D =\r\n    (date.getDate() < 10 ? '0' + date.getDate() + ' ' : date.getDate()) + ' '\r\n  var h =\r\n    date.getHours() < 10 ? '0' + date.getHours() + ':' : date.getHours() + ':'\r\n  var m =\r\n    date.getMinutes() < 10\r\n      ? '0' + date.getMinutes() + ':'\r\n      : date.getMinutes() + ':'\r\n  var s = date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds()\r\n  return Y + M + D + h + m + s\r\n})\r\n\r\n//全局组件\r\nimport MyMenu from './components/MyMenu'\r\nVue.component(MyMenu.name, MyMenu)\r\nimport MyList from './components/MyList'\r\nVue.component(MyList.name, MyList)\r\n\r\nVue.config.productionTip = false\r\n\r\nnew Vue({\r\n  router,\r\n  store,\r\n  render: h => h(App)\r\n}).$mount('#app')\r\n"],"mappings":";;;;;;;;AACA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,GAAG,MAAM,WAAW;AAC3B,OAAOC,MAAM,MAAM,UAAU;AAC7B,OAAOC,KAAK,MAAM,SAAS;AAC3B,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,SAAS,MAAM,YAAY;AAClC,OAAO,sCAAsC;AAC7C,OAAOC,WAAW,MAAM,cAAc;AAEtCN,GAAG,CAACO,GAAG,CAACD,WAAW,EAAE;EACnBE,OAAO,EAAE,GAAG;EACZC,OAAO,EAAE,CAAC;EAAE;EACZC,YAAY,EAAE,CACZ,QAAQ,EACR,OAAO,EACP,YAAY,EACZ,QAAQ,EACR,cAAc,EACd,eAAe,EACf,WAAW;AAEf,CAAC,CAAC;AAEFV,GAAG,CAACO,GAAG,CAACF,SAAS,CAAC;AAElBL,GAAG,CAACW,MAAM,CAACC,aAAa,GAAG,KAAK;AAEhCZ,GAAG,CAACa,SAAS,CAACT,KAAK,GAAGA,KAAK;AAC3B;;AAEA;AACAJ,GAAG,CAACa,SAAS,CAACC,SAAS,GAAGC,OAAO,CAAC,gBAAgB,CAAC;;AAEnD;AACA,OAAOC,MAAM,MAAM,UAAU;AAC7BhB,GAAG,CAACO,GAAG,CAACS,MAAM,CAAC;;AAEf;AACAZ,KAAK,CAACa,YAAY,CAACC,OAAO,CAACX,GAAG,CAC5B,UAAAI,MAAM,EAAI;EACR,IAAIQ,KAAK,GAAGC,MAAM,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;EAChD,IAAIH,KAAK,EAAE;IACT;IACAR,MAAM,CAACY,OAAO,CAACC,OAAO,GAAGL,KAAK;IAC9B;IACA;EACF;EACA,OAAOR,MAAM;AACf,CAAC,EACD,UAAAc,KAAK,EAAI;EACP;EACA,OAAOC,OAAO,CAACC,MAAM,CAACF,KAAK,CAAC;AAC9B,CACF,CAAC;AACD;AACAvB,MAAM,CAAC0B,SAAS,CAAC,UAACC,EAAE,EAAEC,IAAI,EAAEC,IAAI,EAAK;EACnCX,MAAM,CAACY,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;AACvB,CAAC,CAAC;;AAEF;AACA9B,MAAM,CAAC+B,aAAa,CAAC,UAACJ,EAAE,EAAEC,IAAI,EAAEC,IAAI,EAAK;EACvC,IAAMG,SAAS,GAAG/B,KAAK,CAACgC,KAAK,CAACC,IAAI,CAACA,IAAI;EACvC;EACA,IAAIP,EAAE,CAACQ,IAAI,CAACC,WAAW,EAAE;IACvB,IAAI,CAACJ,SAAS,EAAE;MACd;MACAhC,MAAM,CAACqC,IAAI,CAAC;QACVC,IAAI,EAAE;MACR,CAAC,CAAC;MACF,IAAIV,IAAI,CAACU,IAAI,IAAI,IAAI,EAAE;QACrB;QACAT,IAAI,CAAC,GAAG,CAAC;QACT;MACF;MACA;MACAA,IAAI,CAAC,KAAK,CAAC;MACX;IACF;EACF;EACAA,IAAI,CAAC,CAAC;AACR,CAAC,CAAC;;AAEF;AACA;AACA/B,GAAG,CAACyC,MAAM,CAAC,YAAY,EAAE,UAAAC,IAA